<!-- 神社 -->

<!-- 厳島神社 -->
<contribution type="picture" id="{FreeTrain-Shika-picture-structure-shrine-shinto-itukushima-01}">
	<picture  src="shrine_itukushima_01.bmp">
		<override when="night" src="shrine_itukushima_01n.bmp"/>
	</picture>
</contribution>

<!-- 厳島神社 -->
<contribution type="GenericStructure" id="{FreeTrain-Shika-structure-shrine-shinto-itukushima}">
	<group>有名寺社</group>
	<name>厳島神社(3x5)</name>
	<design>厳島神社</design>
	<structure>
		<category byname="有名寺社"/>
	</structure>
	<price>10000</price>
	<height>1</height>
	<size>3,5</size>
	<population>
		<class name="freetrain.contributions.population.CombinationPopulation"/>
		<population>
			<class name="freetrain.contributions.population.ShopperPopulation"/>
			<base>500</base>
		</population>
		<population>
			<class name="freetrain.contributions.population.OfficePopulation"/>
			<base>20</base>
		</population>
	</population>

	<computerCannotBuild />

	<sprite origin="0,0" offset="32">
		<picture ref="{FreeTrain-Shika-picture-structure-shrine-shinto-itukushima-01}"/>
	</sprite>

	<sprite origin="128,0" offset="48" opposite="true">
		<picture ref="{FreeTrain-Shika-picture-structure-shrine-shinto-itukushima-01}"/>
	</sprite>

	<sprite origin="256,0" offset="32">
		<picture ref="{FreeTrain-Shika-picture-structure-shrine-shinto-itukushima-01}"/>
	</sprite>

	<sprite origin="384,0" offset="48" opposite="true">
		<picture ref="{FreeTrain-Shika-picture-structure-shrine-shinto-itukushima-01}"/>
	</sprite>

</contribution>

