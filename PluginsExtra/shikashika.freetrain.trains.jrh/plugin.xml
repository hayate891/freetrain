			<car char="予" ref="{FreeTrain-Shika-PassengerCar-TrainCar-kaya27-501}"/>

			<composition>気</composition>
			<composition>気交</composition>
			<composition>気展交</composition>
			<composition>気展食交</composition>
			<composition>気展食百交</composition>
