	<contribution type="train" id="{FreeTrain-Shika-RailroadTrain-DD51-hokuto-Cassiopeia-1}">
		<company>JR北海道</company>
		<type>DD 51</type>
		<name>カシオペア</name>
		<author>しかしか</author>
		<description>ディーゼル機関車DD51(北斗星色)が牽引する、オール2階建寝台列車です。
		</description>
		<price>100</price>
		<speed>medium</speed>
		<class name="freetrain.contributions.train.PatternTrainImpl"/>
		<config>
			<car char="気" ref="{FreeTrain-Shika-DieselLocomotive-TrainCar-DD51-hokuto}"/>
