<contribution type="picture" id="{FreeTrain-Shika-picture-roadslope-woodlandpath-001}">
	<picture src = "roadslope_woodlandpath_spring.bmp">
		<override when="spring" src="roadslope_woodlandpath_spring.bmp"/>
		<override when="summer" src="roadslope_woodlandpath_summer.bmp"/>
		<override when="autumn" src="roadslope_woodlandpath_autumn.bmp"/>
		<override when="winter" src="roadslope_woodlandpath_winter.bmp"/>
	</picture>
</contribution>

<contribution type="GenericStructure" id="{FreeTrain-Shika-structure-roadslope-woodlandpath-001}">
	<group>道路関連物</group>
	<name>勾配道路</name>
	<design>林道(未舗装)</design>
	<structure>
		<category byname="道路関連物"/>
	</structure>
	<price>500</price>
	<size>1,4</size>
	<height>1</height>
	<population>
		<class name="freetrain.contributions.population.CombinationPopulation"/>
		<population>
			<class name="freetrain.contributions.population.OfficePopulation"/>
			<base>0</base>
		</population>
	</population>

	<computerCannotBuild />

	<sprite origin="0,0" offset="16">
		<picture ref="{FreeTrain-Shika-picture-roadslope-woodlandpath-001}"/>
	</sprite>

	<sprite origin="80,0" offset="40" opposite="true">
		<picture ref="{FreeTrain-Shika-picture-roadslope-woodlandpath-001}"/>
	</sprite>

	<sprite origin="160,0" offset="16">
		<picture ref="{FreeTrain-Shika-picture-roadslope-woodlandpath-001}"/>
	</sprite>

	<sprite origin="240,0" offset="40" opposite="true">
