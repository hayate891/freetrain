<price>2000</price>
<speed>slow</speed>
<class name="freetrain.contributions.train.PatternTrainImpl"/>
<config>
<car char="前" ref="{6F2F2667-05F0-4595-9D44-TYPE1500}-A"/>
<car char="中" ref="{6F2F2667-05F0-4595-9D44-TYPE1500}-C"/>
