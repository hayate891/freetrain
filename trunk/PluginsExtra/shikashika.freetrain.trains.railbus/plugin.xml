<!-- 列車定義 -->
	<contribution type="train" id="{FreeTrain-Shika-RailroadTrain-kiha01-railbus}">
		<company>Old Japanese National Railway</company>
		<type>KIHA01</type>
		<name>RailBus</name>
		<author>Shikashika</author>
		<description>A small sized diesel RailBus (12 were built in 1954). Model numbers KIHA10000～11 were built first. In 1957, KIHA10000～03 became KIHA01 (#01 ~ #04), KIHA10004～11 became KIHA01 (#51～#58).
		</description>
		<price>100</price>
		<speed>slow</speed>
		<class name="freetrain.contributions.train.PatternTrainImpl"/>
		<config>
			<car char="0" ref="{FreeTrain-Shika-DieselRailcar-RailBus-TrainCar-kiha01}"/>
			
			<composition>0</composition>
			<composition>00</composition>
			<composition>000</composition>
		</config>
	</contribution>
	<contribution type="train" id="{FreeTrain-Shika-RailroadTrain-kiyuni01-railbus}">
		<company>Old Japanese National Railway</company>
		<type>Kuyuni01</type>
		<name>RailBus</name>
		<author>Shikashika</author>
		<description>In 1952, KIHA01 - #55 had it's interior remodelled and became Kuyuni01-1 Only one Kuyuni was built. It was used on the Sainhon Line, stopping in 1967.
		</description>
		<price>100</price>
		<speed>slow</speed>
		<class name="freetrain.contributions.train.PatternTrainImpl"/>
		<config>
			<car char="1" ref="{FreeTrain-Shika-DieselRailcar-RailBus-TrainCar-kiyuni01}"/>
			<car char="0" ref="{FreeTrain-Shika-DieselRailcar-RailBus-TrainCar-kiha01}"/>
			
			<composition>1</composition>
			<composition>10</composition>
			<composition>100</composition>
		</config>
	</contribution>
	<contribution type="train" id="{FreeTrain-Shika-RailroadTrain-kiha02-railbus}">
		<company>Old Japanese National Railway</company>
		<type>KIHA02</type>
		<name>RailBus</name>
		<author>Shikashika</author>
		<description>This is a small-sized diesel RailBus. There were only 17 of the KIHA02 Series produced. From the beginning, they were numbered KIHA10012～28, but in 1957, they became KIHA02 (#01～#17)
		</description>
		<price>100</price>
		<speed>slow</speed>
		<class name="freetrain.contributions.train.PatternTrainImpl"/>
